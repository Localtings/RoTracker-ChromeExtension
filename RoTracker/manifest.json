{
  "manifest_version": 3,

  "name": "RoTracker",
  "content_scripts": [{
    "js": [
      "jquery-3.1.0.min.js",
      "trigger.js"
    ],
    "matches": ["http://*/*", "https://*/*"]
  }],

  "host_permissions": [
    "*://*.roblox.com/*"
  ],

  "permissions": [
    "cookies"
  ],

  "description": "Track Profile Status.",
  "version": "1.0",
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png",
    "16": "icon16.png"
  },

  "action": {
    "default_icon": "icon16.png",
    "default_popup": "popup.html"
  }

}